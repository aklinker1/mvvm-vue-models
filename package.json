    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.9",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.1.3",
    "vite": "^2.1.5",
    "vue": "^3.0.5",
    "vue-router": "^4.0.6",
    "vue-tsc": "^0.0.15"
  },
  "peerDependencies": {
    "vue": "3.x"
  }
