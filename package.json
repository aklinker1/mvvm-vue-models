{
  "name": "mvvm-vue-models",
  "version": "0.3.0",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue.js",
    "state",
    "mvvm",
    "composition"
  ],
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "main": "./dist/mvvm-vue-models.umd.js",
